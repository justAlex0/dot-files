<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1646887254644152-1</unique-id>
	<command>alacritty --working-directory %f</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>gvim</icon>
	<name>Open with Vim</name>
	<submenu></submenu>
	<unique-id>1647688622646921-1</unique-id>
	<command>alacritty -e vim %f</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>edit-copy</icon>
	<name>Copy path</name>
	<submenu></submenu>
	<unique-id>1686229957969498-1</unique-id>
	<command>readlink -fn %f | if [[ -n &quot;$WAYLAND_DISPLAY&quot; ]]; then wl-copy; else xclip -selection clipboard; fi
</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>link</icon>
	<name>Follow link</name>
	<submenu></submenu>
	<unique-id>1679673884398275-1</unique-id>
	<command>thunar &quot;$(realpath %f)&quot;</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
